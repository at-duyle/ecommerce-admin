<p id="notice"><%= notice %></p>

<h1>Admins</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password digest</th>
      <th>Email</th>
      <th>Name</th>
      <th>Gender</th>
      <th>Role</th>
      <th>Auth token</th>
      <th>Confirm send at</th>
      <th>Confirm token</th>
      <th>Confirm at</th>
      <th>Reset send at</th>
      <th>Reset token</th>
      <th>Manager</th>
      <th>Available</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.username %></td>
        <td><%= admin.password_digest %></td>
        <td><%= admin.email %></td>
        <td><%= admin.name %></td>
        <td><%= admin.gender %></td>
        <td><%= admin.role %></td>
        <td><%= admin.auth_token %></td>
        <td><%= admin.confirm_send_at %></td>
        <td><%= admin.confirm_token %></td>
        <td><%= admin.confirm_at %></td>
        <td><%= admin.reset_send_at %></td>
        <td><%= admin.reset_token %></td>
        <td><%= admin.manager %></td>
        <td><%= admin.available %></td>
        <td><%= link_to 'Show', admin %></td>
        <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
        <td><%= link_to 'Destroy', admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_path %>
